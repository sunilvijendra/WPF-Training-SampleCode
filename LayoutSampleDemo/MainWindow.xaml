<Window x:Class="LayoutSampleDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LayoutSampleDemo"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">

    <Window.Resources>
        <local:MyProducts x:Key="MyProductList"></local:MyProducts>
    </Window.Resources>

    <Grid>
        
        <ComboBox>
            <ComboBox.Resources></ComboBox.Resources>
        </ComboBox>
        <ComboBox Name="cmbProducts" Margin="156,29,109,258"  Loaded="cmbProducts_Loaded">
 
        </ComboBox>
        <Label Content="Select Product" HorizontalAlignment="Left" Margin="44,29,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
